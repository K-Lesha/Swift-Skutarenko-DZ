var blackKnight1: Figures = .Knight(color: .Black, coordinate: ("a", 2), icon: "üêà‚Äç‚¨õ")
var blackKnight2: Figures = .Knight(color: .Black, coordinate: ("a", 7), icon: "üêà‚Äç‚¨õ")
var blackRook1: Figures = .Rook(color: .Black, coordinate: ("a", 1), icon: "üèØ")
var blackRook2: Figures = .Rook(color: .Black, coordinate: ("a", 8), icon: "üèØ")
var blackPawn1: Figures = .Pawn(color: .Black, coordinate: ("b", 1), icon: "üï¥üèø")
var blackPawn2: Figures = .Pawn(color: .Black, coordinate: ("b", 2), icon: "üï¥üèø")
