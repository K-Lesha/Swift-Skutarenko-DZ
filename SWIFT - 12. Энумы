var blackKing: Figures = .King(color: .Black, coordinate: ("a", 4), icon: "ü§¥üèø")
var blackQueen: Figures = .Queen(color: .Black, coordinate: ("a", 5), icon: "üë∏üèø")
var blackWBishop: Figures = .wBishop(color: .Black, coordinate: ("a", 6), icon: "üñïüèø")
