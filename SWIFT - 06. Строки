    // использовать цикл for, испольуя весь английский алфавит , найти индекс буквы в алфавите
let alphabet = "abcdefghijklmnopqrstuvwxyz"
var index = 0
let search : Character = "e"

for char in alphabet {
    if search == char {
        print("index of \(search) = \(index)")
    }
    index += 1
}
